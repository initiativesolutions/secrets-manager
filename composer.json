      "Tests\\": "tests/"
    },
    "exclude-from-classmap": ["**/tests/**", "**/command/**", "**/key/**", "**/guard/**"]
  },
  "require": {
    "symfony/yaml": "^6.3",
    "ext-readline": "*",
    "ext-openssl": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10.3"
  },
  "bin": [
    "bin/secretctl"
  ]
}
